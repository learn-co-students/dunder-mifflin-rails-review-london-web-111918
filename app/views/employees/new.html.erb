<h2>Create a new employee</h2>
<%= link_to 'Back', employees_path %>

<%= form_for @employee do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name %>

<%= f.label :last_name %>
<%= f.text_field :last_name %>

<%= f.label :alias %>
<%= f.text_field :alias %>

<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :office %>
<%= f.text_field :office %>

<%= f.label :img_url %>
<%= f.text_field :img_url %>

<%= f.collection_select(:dog_id, @dogs, :id, :name) %>

<%= f.submit %>

<% end %>
